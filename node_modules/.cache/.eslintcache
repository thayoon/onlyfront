rticle>\r\n      ))}\r\n    </ArticleListArea>\r\n  );\r\n};\r\n\r\nfunction Article(props) {\r\n  const [playing, setPlaying] = useState(false);\r\n\r\n  let audio = new Audio(`http://haeun9969.dothome.co.kr/capstone/IT/${props.article._id}.wav`);\r\n\r\n  useEffect(() => {\r\n    playing ? audio.play() : audio.pause();\r\n    return () => audio.pause();\r\n  }, [playing]);\r\n\r\n  function togglePlay() {\r\n    setPlaying((s) => !s);\r\n  }\r\n\r\n  return(\r\n    <ArticleArea>\r\n      <ArticleLink\r\n        to={`/article/${props.type}/${props.article._id}`}\r\n        style={{ textDecoration: \"none\" }}\r\n      >\r\n        <Thumbnail src={props.article.img} alt=\"img\" />\r\n        <Title>{props.article.title}</Title>\r\n      </ArticleLink>\r\n      <div className=\"speaker\" onClick={togglePlay}>\r\n        {playing ? (\r\n          <img className=\"pauseImg\" src={pauseIcon} alt=\"pause\" />\r\n        ) : (\r\n          <img className=\"speakerImg\" src={speaker} alt=\"speaker\" />\r\n        )}{\" \"}\r\n      </div>\r\n    </ArticleArea>\r\n    )\r\n}\r\n\r\nexport default IT;","C:\\bulid\\onlyfront\\src\\style\\palette.js",[],[],{"ruleId":"97","severity":1,"message":"98","line":11,"column":6,"nodeType":"99","endLine":11,"endColumn":8,"suggestions":"100"},{"ruleId":"97","severity":1,"message":"101","line":185,"column":6,"nodeType":"99","endLine":185,"endColumn":15,"suggestions":"102"},{"ruleId":"103","severity":1,"message":"104","line":12,"column":8,"nodeType":"105","messageId":"106","endLine":12,"endColumn":14},{"ruleId":"97","severity":1,"message":"101","line":76,"column":6,"nodeType":"99","endLine":76,"endColumn":15,"suggestions":"107"},"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'articles' and 'getArticles'. Either include them or remove the dependency array.","ArrayExpression",["108"],"React Hook useEffect has a missing dependency: 'audio'. Either include it or remove the dependency array.",["109"],"no-unused-va